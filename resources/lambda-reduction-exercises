(\plus . \two . \one . plus two one) 
(\m.\n.\f.\x. m f (n f x)) 
(\f.\x. f (f x)) 
(\f.\x. f x) 


(\mult . \two . \three . mult two three) 
(\m.\n.\f.\x. m (n f) x) 
(\f.\x. f (f x)) 
(\f.\x. f (f (f x))) 


(\exp . \two . \three . exp two three) 
(\m.\n. m n) 
(\f.\x. f (f x)) 
